   "source": [
    "Name = input('Введите название файла: ')\n",
    "N = int(input('Введите колличество рядов: '))\n",
    "K = int(input('Введите колличество символов: '))\n",
    "D = K * '*' + '\\n'\n",
    "with open (Name,'w') as file:\n",
    "    for _ in range(N):\n",
    "        file.write(D)\n",
    "\n",
    "file.close()"
   ]
